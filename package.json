{
  "name": "resume-builder-2023",
  "version": "1.0.0",
  "main": "index.html",
  "author": "Ryanne Graff",
  "license": "MIT",
  "scripts": {
    "start": "parcel & yarn watch 'yarn build-types-meta' ./src/Types && killall -9 node",
    "build": "yarn build-types-meta && parcel build",
    "build-types-meta": "ts-node ./scripts/build-types-meta.ts",
    "deploy": "yarn build && cp ./CNAME ./dist/CNAME"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.9.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "buffer": "^5.5.0||^6.0.0",
    "parcel": "^2.9.2",
    "path-browserify": "^1.0.0",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@picocss/pico": "^2.0.0-alpha1",
    "browser-fs-access": "^0.34.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3"
  },
  "targets": {
    "default": {
      "source": "src/index.html",
      "distDir": "dist"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
